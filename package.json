{
  "name": "ts-jobspy",
  "version": "1.0.0",
  "description": "TypeScript job scraper for LinkedIn, Indeed, Glassdoor, ZipRecruiter & more - rewritten from python-jobspy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "jobs-scraper",
    "linkedin",
    "indeed",
    "glassdoor",
    "ziprecruiter",
    "bayt",
    "naukri",
    "typescript",
    "job-search",
    "web-scraping"
  ],
  "author": "ts-jobspy contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-jobspy/ts-jobspy.git"
  },
  "homepage": "https://github.com/ts-jobspy/ts-jobspy",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "https-proxy-agent": "^7.0.2",
    "playwright": "^1.40.0",
    "puppeteer": "^24.17.0",
    "socks-proxy-agent": "^8.0.2",
    "tough-cookie": "^4.1.3",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/tough-cookie": "^4.0.2",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
