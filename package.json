{
  "name": "ts-jobspy",
  "version": "1.0.0",
  "description": "TypeScript job scraper for LinkedIn, Indeed, Glassdoor, ZipRecruiter, Google, Bayt, Naukri & BDJobs - A port of python-jobspy",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "jobs-scraper",
    "linkedin",
    "indeed",
    "glassdoor",
    "ziprecruiter",
    "google",
    "bayt",
    "naukri",
    "bdjobs",
    "job-search",
    "web-scraping",
    "typescript"
  ],
  "author": "ts-jobspy contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speedyapply/ts-jobspy.git"
  },
  "bugs": {
    "url": "https://github.com/speedyapply/ts-jobspy/issues"
  },
  "homepage": "https://github.com/speedyapply/ts-jobspy#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "https-proxy-agent": "^7.0.4",
    "socks-proxy-agent": "^8.0.2",
    "turndown": "^7.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/turndown": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
