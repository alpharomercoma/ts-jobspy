{
  "name": "ts-jobspy",
  "version": "1.1.0",
  "description": "TypeScript job scraper for LinkedIn, Indeed, Glassdoor, ZipRecruiter & more - rewritten from python-jobspy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:comprehensive": "jest src/test/comprehensive.test.ts",
    "test:coverage": "jest --coverage",
    "test:performance": "jest --testNamePattern='Performance Tests'",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "precommit": "npm run lint && npm run type-check && npm run test:comprehensive",
    "prepare": "npm run build",
    "release": "changeset",
    "ci:publish": "npm run build && changeset publish --access public"
  },
  "keywords": [
    "jobs-scraper",
    "linkedin",
    "indeed",
    "glassdoor",
    "ziprecruiter",
    "bayt",
    "naukri",
    "typescript",
    "job-search",
    "web-scraping"
  ],
  "author": "Alpha Romer Coma <alpharomercoma@proton.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alpharomercoma/ts-jobspy"
  },
  "homepage": "https://github.com/alpharomercoma/ts-jobspy#readme",
  "bugs": {
    "url": "https://github.com/alpharomercoma/ts-jobspy/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "https-proxy-agent": "^7.0.6",
    "socks-proxy-agent": "^8.0.2",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@eslint/js": "^9.34.0",
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.40.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
